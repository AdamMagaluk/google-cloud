{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name" : "Google Cloud Storage Object Delete",
  "configuration-groups": [
    {
      "label" : "Service Account and Project Information",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "placeholder": "Path to the service account file on local file system"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "project",
          "widget-attributes" : {
            "placeholder": "The project id of the Cloud Storage bucket"
          }
        }
      ]
    },
    {
      "label": "Bucket and Path Information",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Bucket Name",
          "name": "bucket",
          "widget-attributes" : {
            "placeholder": "The bucket to delete objects from"
          }
        },
        {
          "name": "paths",
          "widget-type": "csv",
          "label" : "Objects to Delete",
          "widget-attributes": {}
        }
      ]
    }
  ],
  "outputs": []
}
