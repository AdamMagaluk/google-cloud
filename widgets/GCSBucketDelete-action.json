{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name" : "Google Cloud Storage Object Delete",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "project",
          "widget-attributes" : {
            "default": "auto-detect"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Bucket Name",
          "name": "bucket",
          "widget-attributes" : {
            "placeholder": "Bucket to delete objects from"
          }
        },
        {
          "name": "paths",
          "widget-type": "csv",
          "label" : "Objects to Delete",
          "widget-attributes": {
            "placeholder": "Object to delete"
          }
        }
      ]
    },
    {
      "label" : "Credentials",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "default": "auto-detect"
          }
        }
      ]
    }
  ],
  "outputs": []
}
