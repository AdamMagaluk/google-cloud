{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Google BigQuery Delete",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "project",
          "widget-attributes": {
            "default": "auto-detect"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Dataset Project ID",
          "name": "datasetProject",
          "widget-attributes": {
            "placeholder": "Project the dataset or table belongs to, if different from the Project ID."
          }
        },
        {
          "widget-type": "textbox",
          "label": "Dataset Name",
          "name": "dataset",
          "widget-attributes": {
            "placeholder": "Name of Dataset"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "table",
          "widget-attributes": {
            "placeholder": "Name of Table"
          }
        },
        {
          "name": "deleteContents",
          "label": "Delete Contents",
          "widget-type": "radio-group",
          "widget-attributes": {
            "layout": "inline",
            "default": "false",
            "options": [
              {
                "id": "false",
                "label": "False"
              },
              {
                "id": "true",
                "label": "True"
              }
            ]
          }
        }
      ]
    },
    {
      "label": "Credentials",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes": {
            "default": "auto-detect"
          }
        }
      ]
    }
  ],
  "outputs": []
}
