{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name" : "Google Big Query Table",
  "configuration-groups": [
    {
      "label" : "Service Account and Temporary Google Storage",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "placeholder": "Path to the service account file on local file system"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Bucket Name",
          "name": "bucket",
          "widget-attributes" : {
            "placeholder": "Google Cloud Storage bucket for temporary data"
          }
        },
      ]
    },
    {
      "label": "Dataset Information",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "project",
          "widget-attributes" : {
            "placeholder": "The project id of the dataset"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Dataset",
          "name": "dataset",
          "widget-attributes" : {
            "placeholder": "The dataset the table belongs to"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Table",
          "name": "table",
          "widget-attributes" : {
            "placeholder": "The table to write to"
          },
          "plugin-function": {
            "method": "POST",
            "label": "Extract Schema",
            "widget": "outputSchema",
            "output-property": "schema",
            "plugin-method": "getSchema",
            "position": "bottom",
            "multiple-inputs": true,
            "button-class": "btn-hydrator"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "label": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "boolean",
          "long",
          "double",
          "bytes",
          "string",
          "array"
        ],
        "schema-default-type": "string"
      }
    }
  ]
}
