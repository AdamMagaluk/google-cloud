{
  "metadata": {
    "spec-version": "1.0"
  },
  "display-name": "Google Cloud Storage Blob",
  "configuration-groups": [
    {
      "label" : "Governance",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        }
      ]
    },
    {
      "label" : "Service Account and Project Information",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "placeholder": "Path to the service account file on local file system"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "project",
          "widget-attributes" : {
            "placeholder": "The project id of the Cloud Storage bucket"
          }
        }
      ]
    },
    {
      "label": "Path Information",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Path",
          "name": "path",
          "widget-attributes" : {
            "placeholder": "Path to be processed, specified as gs://<bucket>/<dir>/<dir>"
          }
        }
      ]
    },
    {
      "label" : "Credentials",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "default": "auto-detect"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "path" : "string",
        "body": "bytes"
      }
    }
  ],
  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }
}
